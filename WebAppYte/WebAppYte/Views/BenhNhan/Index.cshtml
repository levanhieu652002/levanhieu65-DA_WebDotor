
@model IEnumerable<WebAppYte.Models.BenhNhan>

@{ ViewBag.Title = "Index";
                Layout = "~/Views/Shared/_Layout.cshtml"; }

<h2>Hồ sơ cá nhân</h2>

<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.Tenbn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Sdt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ngaysinh)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Gioitinh)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tendn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Mk)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Diachi)
        </th>


        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>


    <td>
        @Html.DisplayFor(modelItem => item.Tenbn)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Email)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Sdt)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Ngaysinh)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Gioitinh)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Tendn)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Mk)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Diachi)
    </td>
   
    <td>
        <button class="btn-default">
            @Html.ActionLink("Chỉnh sửa", "Edit", new { id = item.Mabn })
        </button>

        <button class="btn-default">

            @Html.ActionLink("Xem chi tiết", "Details", new { id = item.Mabn })

        </button>

    </td>
</tr>}

</table>
